public String registerStudent(String name, String phone, String email) {
    //name, phone, email 
    //insert in obj apxio__Student__c - name phone and email.
    //return regis num.
    apxio__Student__c.Name=name;
    apxio__Student__c.apxio__Email__c = email;
    apxio__Student__c.apxio__Phone__c = phone;
    insert apxio__Student__c;

    apxio__Registration_Number__c = [Select Id from apxio__Student__c];
    return apxio__Registration_Number__c;


}